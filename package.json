{
  "name": "onedarker-pro",
  "displayName": "One Darker Pro Theme",
  "description": "A darker theme inspired by One Dark Pro, with an improved contrast theme for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "pieersx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pieersx/onedarker-pro"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "One Darker Pro",
        "uiTheme": "vs-dark",
        "path": "./themes/OneDarker-Pro.json"
      }
    ],
    "configuration": {
      "title": "One Darker Pro",
      "properties": {
        "oneDarkerPro.bold": {
          "type": "boolean",
          "default": true,
          "description": "Toggle bold on some scopes"
        },
        "oneDarkerPro.italic": {
          "type": "boolean",
          "default": true,
          "description": "Toggle italics on some scopes"
        },
        "oneDarkerPro.markdownStyle": {
          "type": "boolean",
          "default": true,
          "description": "Toggle Markdown Style"
        },
        "oneDarkerPro.vivid": {
          "type": "boolean",
          "default": true,
          "description": "Toggle vivid colors"
        }
      }
    }
  },
  "icon": "./icon.png",
  "keywords": [
    "one darker",
    "one darker pro",
    "onedarker pro",
    "gruber darker theme",
    "dark",
    "theme"
  ]
}
